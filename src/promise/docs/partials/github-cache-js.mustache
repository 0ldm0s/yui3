<script type="text/javascript">
YUI().use('node', 'jsonp', 'promise', function (Y) {

{{>github-cache}}

var demo = Y.one('#demo');
function renderUser(user) {
    demo.append('<div class="success">Loaded ' + user.login + '</a>\'s data! ' +
        '<a href="' + user.html_url + '">Link to profile</a></div>');
}
function showError(message) {
    demo.append('<div class="error">' + message + '</div>');
}

GitHub.getUser('yui').then(renderUser, showError);
GitHub.getUser('y_ui').then(renderUser, showError);

});
</script>