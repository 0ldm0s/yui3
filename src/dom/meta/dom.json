{
    "dom": {
        "plugins": {
            "selector-css3": {
                "requires": [
                    "selector-native",
                    "selector-css2"
                ]
            },
            "dom-style-ie": {
                "requires": [
                    "dom-style"
                ],
                "condition": {
                   "trigger": "dom-style",
                   "test": "ie-style-test.js"
                }
            },

            "dom-deprecated": {
                "requires": [
                    "dom-base"
                ]
            },

            "selector-css2": {
                "requires": [
                    "selector-native"
                ],
                "condition": {
                   "trigger": "selector",
                   "test": "selector-test.js"
                }
            }
        },
        "requires": [
            "oop"
        ],
        "submodules": {
            "dom-core": {
                "requires": [
                    "oop",
                    "features"
                ]
            },
            "dom-attrs": {
                "requires": [
                    "dom-core"
                ]
            },
            "dom-create": {
                "requires": [
                    "dom-core"
                ]
            },
            "dom-class": {
                "requires": [
                    "dom-core"
                ]
            },
            "dom-size": {
                "requires": [
                    "dom-base"
                ]
            },
            "dom-base": {
                "requires": [
                    "dom-core",
                    "dom-attrs",
                    "dom-create",
                    "dom-class",
                    "dom-size"
                ]
            },
            "dom-screen": {
                "requires": [
                    "dom-core",
                    "dom-style"
                ]
            },
            "dom-style": {
                "requires": [
                    "dom-core"
                ]
            },
            "selector-native": {
                "requires": [
                    "dom-core"
                ]
            },
            "selector": {
                "requires": [
                    "selector-native"
                ]
            }
        }
    }
}
